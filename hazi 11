#include <iostream>
#include <vector>

using namespace std;

int main()
{
    int n,pos,hosz;
    cout << "n=";
    cin >> n;
    vector<int>blu;
    for(int i=0; i<n; i++)
    {
        cout << "blu[" << i << "]";
        int x;
        cin >> x;
        blu.push_back(x);
    }
    cin>>pos;
    cin>>hosz;
    blu.erase(blu.begin()+pos, blu.begin()+hosz+pos);
    int k = blu.size();
    for(int i=0; i<k ; i++ )
    {
        cout<< blu[i]<<" " ;
    }
    return 0;
}
