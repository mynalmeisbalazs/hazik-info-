#include <iostream>
using namespace std;

int main() {
    int n;
    cout<<"n=";
    cin >> n;
    int a[n], b[n];
    int evenSumA = 0, oddSumB = 0;
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    for (int i = 0; i < n; i++) {
        cin >> b[i];
    }

    for (int i = 0; i < n; i++) {
        if (a[i] % 2 == 0) {
            evenSumA += a[i];
        }
    }
    for (int i = 0; i < n; i++) {
        if (b[i] % 2 != 0 && b[i] < evenSumA) {
            oddSumB += b[i];
        }
    }

    cout << oddSumB << endl;
    return 0;
}
